{
  "name": "@apitools/openapi-parser",
  "author": "Mrinmoy Majumdar",
  "license": "MIT",
  "version": "0.0.33",
  "description": "Resolve OpenAPI Refs and/or convert Swagger 2.0 to OpenAPI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrin9/apitools.git",
    "directory": "packages/openapi-parser"
  },
  "module": "dist/openapi-parser.umd.js",
  "main": "dist/openapi-parser.umd.js",
  "exports": {
    ".": {
      "import": "./dist/openapi-parser.mjs",
      "require": "./dist/openapi-parser.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test-debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "swagger-client": "^3.29.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-eslint": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "jest": "^29.7.0",
    "vite": "^5.4.7",
    "typescript": "^5.6.2",
    "rollup-plugin-terser": "^7.0.2"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "openapi",
    "parser",
    "swagger2openapi",
    "swagger-converter",
    "dereference",
    "json",
    "yaml",
    "swagger-parser"
  ]
}
